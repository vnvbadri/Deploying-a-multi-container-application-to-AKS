{
	 "if": {
	    "anyOf": [
		 {
			"not": {
			  "anyOf": [
                    {
				    "field": "type",
				    "like": "Microsoft.ContainerRegistry/registries/*"
				    },
                    {
				    "field": "type",
				    "like": "Microsoft.ContainerService/managedClusters/*"
				    },
                    {
				    "field": "type",
				    "like": "Microsoft.Sql/servers/*"
				    },
                    {
				    "field": "type",
				    "like": "Microsoft.Sql/servers/databases/*"
					},
					{
					"field": "type",
					"like": "Microsoft.Storage/storageAccounts/*"
		            },
                    {
				    "field": "type",
				    "in": ["Microsoft.ContainerRegistry/registries", "Microsoft.ContainerService/managedClusters", "Microsoft.Sql/servers", "Microsoft.Sql/servers/databases", "Microsoft.Storage/storageAccounts"]
				    }
                ]
			}
		 },
          {
		  "allof": [
			{
			  "field": "type",
			  "equals": "Microsoft.ContainerRegistry/registries"
			},
			{
            "field": "Microsoft.ContainerRegistry/registries/sku.name",
            "notIn": ["Standard"]
            }
			]
		  },
         {
		  "allof":[
			{
			  "field": "type",
			  "equals": "Microsoft.SQL/servers/databases"
			},
			{
			  "not":{
					"field": "Microsoft.Sql/servers/databases/requestedServiceObjectiveName",
					"in": ["S0"]
			  }
			}
		  ]
		},
		{
		 "allOf": [
		   {
			 "source": "action",
			 "equals": "Microsoft.Storage/storageAccounts/write"
		   },
		   {
		 	 "field": "type",
			 "equals": "Microsoft.Storage/storageAccounts"
		   },
		   {
		 	 "not": {
			  	    "field": "Microsoft.Storage/storageAccounts/sku.name",
				    "in": [ "Standard_LRS" ]
			 }
		   }
		  ]
		},
		
	  ]
	},
	"then": {
	  "effect": "deny"
	}
}
